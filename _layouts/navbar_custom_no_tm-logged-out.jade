include ../_mixins/links.jade

.fixed.landing-page
    if(global.custom.custom_navigation.NoTm)
        #team-mentor-navigation(class=global.custom.custom_navigation.navbarclass)
            a(href='/jade/user/main.html')
                #tm-logo(class=global.custom.custom_navigation.logoclass)
                    img(src=global.custom.custom_navigation.logo)

            p(style=global.custom.custom_navigation.logoTextStyle)
                span #{global.custom.custom_navigation.logoText}

            p(style=global.custom.custom_navigation.logoSubTextStyle)
                span #{global.custom.custom_navigation.logoSubText}